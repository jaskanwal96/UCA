#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int a[] = {1,2,3,4,5,6,7,8};
    int search,flag=0;
    scanf("%d",&search);
    int size = sizeof(a)/sizeof(a[0]);

    int mid,l = 0,r = size-1;
    while( l<=r){
        
      mid=(l+r)/2;
        
         if(search == a[mid])
            {
             
            printf("%d found at index %d\n",search,mid);flag=1;
            break;
        }
        if(search > a[mid])
            {
            
       l=mid+1; 
        }
        else
            {
            
            r=mid-1;
        }
    
       
        
    }
   if(flag==0)
       printf("Not Found");
    return 0;
}
